<assembly xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
<id>jar-with-guava-hive-dependencies</id>
<formats>
<format>jar</format>
</formats>
<includeBaseDirectory>false</includeBaseDirectory>
<fileSets><fileSet>
<directory>target/classes</directory>
<outputDirectory/><includes><include>**</include>
</includes>
<useDefaultExcludes>true</useDefaultExcludes>
<fileMode>0644</fileMode>
<directoryMode>0755</directoryMode>
</fileSet>
</fileSets>
<dependencySets><dependencySet><scope>runtime</scope><unpack>true</unpack><useProjectArtifact>false</useProjectArtifact><includes>
<include>com.google.guava:guava</include>
<include>org.apache.hive:hive-exec</include></includes>
</dependencySet>
</dependencySets>
</assembly>
